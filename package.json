{
    "name": "kickbase-client",
    "version": "0.0.0-development",
    "main": "src/Client.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/DasRed/kickbase-client.git"
    },
    "dependencies": {
        "moment": "^2.29.4",
        "node-fetch": "^3.2.10"
    },
    "files": [
        "src/*"
    ],
    "devDependencies": {
        "semantic-release-cli": "^5.4.4",
        "semantic-release": "^19.0.5"
    },
    "scripts": {
        "semantic-release": "semantic-release --ci"
    },
    "publishConfig": {
        "access": "restricted"
    },
    "release": {
        "branches": [
            "main"
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/github"
        ],
        "prepare": [
            "@semantic-release/npm",
            {
                "path": "@semantic-release/git",
                "assets": [
                    "package.json",
                    "package-lock.json"
                ]
            }
        ]
    }
}
